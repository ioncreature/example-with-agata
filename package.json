{
  "name": "example-with-agata",
  "version": "1.0.0",
  "description": "Example chat application with Agata library",
  "main": "index.js",
  "scripts": {
    "coverage": "jest --runInBand --coverage",
    "dev": "./index.js dev",
    "env": "sh script/run-deps.sh",
    "lint": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ioncreature/example-with-agata.git"
  },
  "author": "Aleksandr Marenin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ioncreature/example-with-agata/issues"
  },
  "homepage": "https://github.com/ioncreature/example-with-agata#readme",
  "dependencies": {
    "agata": "2.3.6",
    "body-parser": "1.19.0",
    "commander": "6.2.1",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "glob": "7.1.6",
    "ioredis": "4.19.4",
    "lodash": "4.17.20",
    "mongoose": "5.11.8",
    "morgan": "1.10.0",
    "supertest": "6.0.1",
    "winston": "3.3.3",
    "ws": "7.4.1",
    "yahel": "0.5.3"
  },
  "devDependencies": {
    "eslint": "7.16.0",
    "eslint-config-agata": "0.2.1",
    "jest": "26.6.3"
  },
  "eslintConfig": {
    "extends": "agata",
    "env": {
      "node": true
    }
  },
  "jest": {
    "testTimeout": 3000,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "collectCoverage": false,
    "coverageReporters": [
      "json",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/cleanup.js"
    ]
  }
}
